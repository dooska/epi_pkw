

<h1>Listing Votes</h1>

<table>
  <thead>
    <tr>
      <th>Committee</th>
      <th>Constituency</th>
      <th>Yes vote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.committee %></td>
        <td><%= vote.constituency %></td>
        <td><%= vote.yes_vote %></td>
        <td><%= link_to 'Show', vote %></td>
        <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
        <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_vote_path %>
